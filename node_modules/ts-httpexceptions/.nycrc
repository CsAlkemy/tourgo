{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/index.ts",
    "**/index.js",
    "**/interfaces/*.js"
  ],
  "reporter": [
    "text-summary",
    "html",
    "lcov"
  ],
  "extension": [
    ".ts"
  ],
  "check-coverage": true,
  "lines": 99,
  "statements": 100,
  "functions": 99,
  "branches": 89,
  "all": true
}
